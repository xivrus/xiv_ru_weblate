				<target state="final" xml:space="preserve">TEXT_VOICEMAN_05401_000010_GAIA&lt;tab&gt;Préparez-vous, j'appelle les Ténèbres&lt;var 1D /var&gt;!</target>
			</trans-unit>
			<trans-unit id="1" approved="yes">
				<source>1</source>
				<target state="final" xml:space="preserve">TEXT_VOICEMAN_05401_000020_RYNE&lt;tab&gt;Bravo Gaïa, tu as réussi&lt;var 1D /var&gt;!</target>
			</trans-unit>
			<trans-unit id="2" approved="yes">
				<source>2</source>
				<target state="final" xml:space="preserve">TEXT_VOICEMAN_05401_000030_RYNE&lt;tab&gt;Une bataille vieille d'un siècle...</target>
