      <trans-unit id="24" xml:space="preserve" approved="yes">
        <source>24</source>
        <target state="final">TEXT_LUCKZI008_03544_TODO_00&lt;tab&gt;盛られた土を調べる</target>
      </trans-unit>
      <trans-unit id="25" xml:space="preserve" approved="yes">
