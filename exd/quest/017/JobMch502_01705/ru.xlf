      <trans-unit id="0" xml:space="preserve" approved="no">
        <source>0</source>
        <target state="translated">TEXT_JOBMCH502_01705_SEQ_00&lt;tab&gt;Стефанивьян желает отпраздновать вашу победу в турнире.</target>
      </trans-unit>
      <trans-unit id="1" xml:space="preserve" approved="no">
        <source>1</source>
