      <trans-unit id="10" xml:space="preserve" approved="yes">
        <source>10</source>
        <target state="final">TEXT_WARPINNISHGARD_Q2_000_1&lt;tab&gt;Que voulez-vous faire&lt;var 1D /var&gt;?</target>
      </trans-unit>
      <trans-unit id="11" xml:space="preserve" approved="yes">
        <source>11</source>
        <target state="final">TEXT_WARPINNISHGARD_A2_000_1&lt;tab&gt;Aller dans ma chambre</target>
      </trans-unit>
      <trans-unit id="12" xml:space="preserve" approved="yes">
