      </trans-unit>
      <trans-unit id="1" xml:space="preserve" approved="yes">
        <source>1</source>
        <target state="final">TEXT_REGDRA2ANYXTRINE_00285_Q1_000_000&lt;tab&gt;何を聞く？</target>
      </trans-unit>
      <trans-unit id="2" xml:space="preserve" approved="yes">
