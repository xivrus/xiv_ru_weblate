        <target state="final">TEXT_CMNBHTREWARDLV020ST0003_00025_AFTER_RWD_RETURN_MENU_NO&lt;tab&gt;No</target>
      </trans-unit>
      <trans-unit id="22" xml:space="preserve" approved="yes">
        <source>22</source>
        <target state="final">TEXT_CMNBHTREWARDLV020ST0003_00025_FIRST_BONUS_TALK_00&lt;tab&gt;The operation was a success! Here's your payment, and as promised, I've thrown in a little extra, so that you may consider returning to aid us once again.</target>
      </trans-unit>
      <trans-unit id="23" xml:space="preserve" approved="yes">
        <source>23</source>
        <target state="final">TEXT_CMNBHTREWARDLV020ST0003_00025_FIRST_BONUS_SYSTEM_TALK_00&lt;tab&gt;A bonus has been added to your reward.</target>
