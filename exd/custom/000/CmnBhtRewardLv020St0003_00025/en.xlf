        <target state="final">TEXT_CMNBHTREWARDLV020ST0003_00025_AFTER_RWD_MENU_YES&lt;tab&gt;Yes</target>
      </trans-unit>
      <trans-unit id="18" xml:space="preserve" approved="yes">
        <source>18</source>
        <target state="final">TEXT_CMNBHTREWARDLV020ST0003_00025_AFTER_RWD_MENU_NO&lt;tab&gt;No</target>
      </trans-unit>
      <trans-unit id="19" xml:space="preserve" approved="yes">
        <source>19</source>
        <target state="final">TEXT_CMNBHTREWARDLV020ST0003_00025_AFTER_RWD_RETURN_MENU_TITLE&lt;tab&gt;You will be removed from the party. Proceed?</target>
      </trans-unit>
      <trans-unit id="20" xml:space="preserve" approved="yes">
        <source>20</source>
        <target state="final">TEXT_CMNBHTREWARDLV020ST0003_00025_AFTER_RWD_RETURN_MENU_YES&lt;tab&gt;Yes</target>
      </trans-unit>
      <trans-unit id="21" xml:space="preserve" approved="yes">
